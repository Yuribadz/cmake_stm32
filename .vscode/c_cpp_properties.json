{
    "version": 4,
    "configurations": [
        {
            "name": "Linux-arm-cross-platform",
            "includePath": [
                "${workspaceFolder}/**",
                "~/STM32Cube_FW_F3_V1.10.0/**",
                "~/STM32Cube_FW_F3_V1.10.0/Drivers/STM32F3xx_HAL_Driver/Inc/**"
            ],
            "defines": ["STM32F303RE","USE_HAL_DRIVER","\"_DEBUG\",",
                "\"UNICODE\",",
                "\"_UNICODE\""],
            "compilerPath": "arm-none-eabi-g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "intelliSenseMode": "gcc-arm",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ]   
}
