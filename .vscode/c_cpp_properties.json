{
    "version": 4,
    "configurations": [
        {
            "name": "Linux-arm-cross-platform",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/inc",
                "~/STM32CubeF3-master/Drivers/STM32F3xx_HAL_Driver/Inc/Legacy",
                "~/STM32CubeF3-master/Drivers/STM32F3xx_HAL_Driver/Inc",
                "~/STM32CubeF3-master/Drivers/STM32F3xx_HAL_Driver/Src"
            ],
            "defines": ["STM32F303RE","USE_HAL_DRIVER",
                "USE_DEVICE_MODE",
                "USE_FULL_ASSERT"],
            "compilerPath": "arm-none-eabi-g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ]   
}
